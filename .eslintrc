{
  parser: "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "max-len": [0, 120, 2, {ignoreComments: true}],
    "no-param-reassign": [0],
    "radix": [0],
    "camelcase": [0],
    "no-use-before-define": [0],
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "func-names": [0],
    "no-console": [0]
  },
  "env": {
    "node": true,
    "mocha": true
  },
}
